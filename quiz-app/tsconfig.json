{
     "include": [
         "./src/**/*"
     ],
     "compilerOptions": {
         "noImplicitAny": false,
         "moduleResolution": "node",
         "allowJs": true,
         "strict": true,
         "esModuleInterop": true,
         "lib": [
             "dom",
             "es2015"
         ],
         "jsx": "react",
         "target": "es5",
         "skipLibCheck": true,
         "allowSyntheticDefaultImports": true,
         "forceConsistentCasingInFileNames": true,
         "module": "esnext",
         "resolveJsonModule": true,
         "isolatedModules": true,
         "noEmit": true
     }
 }